                                                    <Grid.InputBindings>
                                                        <MouseBinding Gesture="LeftDoubleClick"  Command="{Binding Path=DataContext.LoadSpectralClasses, RelativeSource={RelativeSource AncestorType={x:Type Window}}}"/>
                                                    </Grid.InputBindings>

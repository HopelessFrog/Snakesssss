                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="*" />

                                                    </Grid.ColumnDefinitions>
                                                    <StackPanel Orientation="Horizontal" Grid.Column="0">

                                                        <Image Source="{Binding ImagePath}" Width="100" Height="100" />
                                                        <StackPanel
                                                            Margin="8,0,0,0">
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock
                                                                    Margin="10,10,0,10"
                                                                    FontWeight="Bold"
                                                                    FontStretch="Normal"
                                                                    Text="Name: " >
                                                                    <TextBlock.InputBindings>
                                                                        <MouseBinding MouseAction="LeftDoubleClick"
                                                                            Command="{Binding DataContext.LoadSpectralClasses, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}"/>
                                                                    </TextBlock.InputBindings>
                                                                </TextBlock>
                                                                <TextBlock
                                                                    Margin="0,10,10,10"
                                                                    FontWeight="Bold"
                                                                    FontStretch="Normal"
                                                                    Text="{Binding Name}" />
                                                            </StackPanel>
                                                           <StackPanel Orientation="Horizontal">
                                                               <TextBlock
                                                                   Margin="10,10,0,10"
                                                                   FontStretch="Normal"
                                                                   Text="Family: " />
                                                               <TextBlock
                                                                   Margin="0,10,10,10"
                                                                   FontStretch="Normal"
                                                                   Text="{Binding Name}" />
                                                            </StackPanel>
                                                           
                                                        </StackPanel>

                                                    </StackPanel>
                                                    <Grid Grid.Column="1">
                                                        <StackPanel Orientation="Horizontal"
                                                                    HorizontalAlignment="Right">
                                                            <Button Content="Edit " Margin="5" />
                                                            <Button Content="Delete" Margin="5" Background="Red" />
                                                        </StackPanel>

                                                    </Grid>
                                                </Grid>
                                            </materialDesign:Card>

                                        </DataTemplate>
                                    </ListBox.ItemTemplate>
                                </ListBox>-->
                            </Grid>

                            <materialDesign:Card>
                                <Grid HorizontalAlignment="Stretch">

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="70*" />

                                        <RowDefinition Height="326*" />

                                    </Grid.RowDefinitions>

                                    <Grid>
                                        <Viewbox HorizontalAlignment="Left">

                                            <StackPanel Orientation="Horizontal">
                                                <TextBox
                                                    HorizontalAlignment="Stretch"
                                                    VerticalAlignment="Center"
                                                    Margin=" 10 "
                                                    materialDesign:HintAssist.Hint="Поиск....................................."
                                                    materialDesign:HintAssist.Foreground="Green"
                                                    materialDesign:TextFieldAssist.UnderlineBrush="Green"
                                                    Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                                    Text="{Binding SnakeForSearch.Name}" />
                                                <Button
                                                    Command="{Binding Search}"
                                                    Style="{StaticResource MaterialDesignToolButton}">
                                                    <materialDesign:PackIcon
                                                        Margin="8,0,8,0"
                                                        Kind="Search"
                                                        Opacity=".56" />
                                                </Button>


                                            </StackPanel>


                                        </Viewbox>


                                    </Grid>
                                    <Grid Grid.Row="2">

                                        <Viewbox HorizontalAlignment="Left">


                                            <ScrollViewer Margin="15" HorizontalAlignment="Left"
                                                          HorizontalScrollBarVisibility="Disabled"
                                                          VerticalScrollBarVisibility="Auto">
                                                <Grid HorizontalAlignment="Stretch">


                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />


                                                    </Grid.RowDefinitions>


                                                    <Viewbox Grid.Row="0" Margin="5,0,5,0">
                                                        <StackPanel Orientation="Horizontal"
                                                                    HorizontalAlignment="Center">
                                                            <Label FontSize="26" Padding="0,15,14,0">Длинна:</Label>
                                                            <TextBox
                                                                Margin="0,0,10,0"
                                                                FontSize="20"
                                                                materialDesign:HintAssist.Hint="Мин"
                                                                materialDesign:HintAssist.IsFloating="True"
                                                                Text="{Binding SnakeForSearch.MinLenght}" />
                                                            <TextBox
                                                                Margin="0,0,10,0"
                                                                FontSize="20"
                                                                materialDesign:HintAssist.Hint="Макс"
                                                                materialDesign:HintAssist.IsFloating="True"
                                                                Text="{Binding SnakeForSearch.MaxLenght}" />

                                                        </StackPanel>


                                                    </Viewbox>
                                                    <Viewbox Grid.Row="5" Margin="5,0,5,0">
                                                        <StackPanel Orientation="Horizontal"
                                                                    HorizontalAlignment="Center"
                                                                    IsEnabled="{Binding SelectedPoisonType}">
                                                            <TextBlock TextWrapping="Wrap" Padding="0,15,14,0"
                                                                       FontSize="20">
                                                                Количество <LineBreak /> яда:
                                                            </TextBlock>
                                                            <TextBox
                                                                FontSize="20"
                                                                Margin="0,0,10,0"
                                                                materialDesign:HintAssist.Hint="Мин"
                                                                materialDesign:HintAssist.IsFloating="True"
                                                                Text="{Binding SnakeForSearch.MinAmountOfPoison}" />
                                                            <TextBox
                                                                FontSize="20"
                                                                Margin="0,0,10,0"
                                                                materialDesign:HintAssist.Hint="Макс"
                                                                materialDesign:HintAssist.IsFloating="True"
                                                                Text="{Binding SnakeForSearch.MaxAmountOfPoison}" />

                                                        </StackPanel>


                                                    </Viewbox>
                                                    <Viewbox Grid.Row="1" Margin="5">

                                                        <xctk:CheckComboBox
                                                            x:Name="AreasBoxSearch"
                                                            ItemSelectionChanged="AreasBox_OnItemSelectionChangedSearch"
                                                            FontSize="20"
                                                            materialDesign:HintAssist.Hint="Ареал"
                                                            materialDesign:HintAssist.IsFloating="True"
                                                            ItemsSource="{Binding Areas}"
                                                            Width="200" />

                                                    </Viewbox>
                                                    <Viewbox Grid.Row="2" Margin="5">
                                                        <xctk:CheckComboBox
                                                            x:Name="ColorsBoxSearch"
                                                            FontSize="20"
                                                            ItemSelectionChanged="ColorsBox_OnItemSelectionChangedSearch"
                                                            materialDesign:HintAssist.Hint="Цвета"
                                                            materialDesign:HintAssist.IsFloating="True"
                                                            ItemsSource="{Binding Colors}"
                                                            Width="200">
                                                            <xctk:CheckComboBox.ItemTemplate>
                                                                <DataTemplate>
                                                                    <StackPanel Orientation="Horizontal">
                                                                        <TextBlock Text="{Binding Name}" />
                                                                        <Ellipse Margin="3,0,3,0" Width="15"
                                                                            Fill="{Binding Color}" />
                                                                    </StackPanel>
                                                                </DataTemplate>
                                                            </xctk:CheckComboBox.ItemTemplate>
                                                        </xctk:CheckComboBox>

                                                    </Viewbox>
                                                    <Viewbox Grid.Row="3" Margin="5,0,5,0">
                                                        <ComboBox Margin="0" Padding="0"
                                                                  SelectedIndex="0"
                                                                  materialDesign:HintAssist.Hint="Семейство"
                                                                  materialDesign:HintAssist.IsFloating="True"
                                                                  FontSize="20" Width="200" Height="60"
                                                                  ItemsSource="{Binding Familyes}"
                                                                  SelectedItem="{Binding SnakeForSearch.Family}" />
                                                    </Viewbox>
                                                    <Viewbox Grid.Row="4" Margin="5,0,5,0">
                                                        <ComboBox Margin="0" Padding="0"
                                                                  SelectedIndex="0"
                                                                  materialDesign:HintAssist.Hint="Тип яда"
                                                                  materialDesign:HintAssist.IsFloating="True"
                                                                  FontSize="20" Width="200" Height="60"
                                                                  ItemsSource="{Binding PoisonTypes}"
                                                                  SelectedItem="{Binding PoisonTypeSearch}" />
                                                    </Viewbox>
                                                    <Viewbox Grid.Row="6" Margin="5,0,5,0">
                                                        <ComboBox Margin="0" Padding="0"
                                                                  SelectedIndex="0"
                                                                  materialDesign:HintAssist.Hint="Узор"
                                                                  materialDesign:HintAssist.IsFloating="True"
                                                                  FontSize="20" Width="200" Height="60"
                                                                  ItemsSource="{Binding Designs}"
                                                                  SelectedItem="{Binding SnakeForSearch.Design}" />
                                                    </Viewbox>


                                                </Grid>
                                            </ScrollViewer>
                                        </Viewbox>
                                    </Grid>


                                </Grid>
                            </materialDesign:Card>
                        </Grid>
                    </materialDesign:Card>
                </TabItem>
                <TabItem IsEnabled="True" Visibility="Visible">
                    <TabItem.Header>
                        <StackPanel
                            Width="auto"
                            Height="auto">
                            <materialDesign:PackIcon
                                Width="40"
                                Height="40"
                                HorizontalAlignment="Center"
                                Kind="Image" />
                            <TextBlock
                                HorizontalAlignment="Center"
                                Text="PicSearch" />
                        </StackPanel>
                    </TabItem.Header>
                    <Viewbox>
                        <materialDesign:Card Margin="4">
                            <Grid>
                                <ProgressBar
                                    Visibility="{Binding Loading}"
                                    IsIndeterminate="True"
                                    Style="{StaticResource MaterialDesignCircularProgressBar}"
                                    Value="50" />
                                <materialDesign:Card Padding="8" Margin="5" Visibility="{Binding SnakeRdy}">
                                    <materialDesign:Flipper
                                        materialDesign:ElevationAssist.Elevation="Dp0"
                                        Style="{StaticResource MaterialDesignCardFlipper}">
                                        <materialDesign:Flipper.FrontContent>
                                            <Grid
                                                Width="200"
                                                Height="306">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition
                                                        Height="160" />
                                                    <RowDefinition
                                                        Height="*" />
                                                </Grid.RowDefinitions>
                                                <materialDesign:ColorZone
                                                    VerticalAlignment="Stretch"
                                                    Mode="PrimaryLight">
                                                    <Image Source="{Binding AiSnake.ImagePath}" Width="160"
                                                           Height="160" />
                                                </materialDesign:ColorZone>
                                                <StackPanel
                                                    Grid.Row="1"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">
                                                    <StackPanel Orientation="Horizontal" Margin="2">
                                                        <TextBox
                                                            Text="{Binding AiSnake.Name}" FontWeight="Bold"
                                                            FontStretch="Normal" />
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal" Margin="2">
                                                        <TextBox
                                                            materialDesign:HintAssist.Hint="Family"
                                                            materialDesign:HintAssist.IsFloating="True"
                                                            Text="{Binding AiSnake.Family.Name}" />
                                                    </StackPanel>

                                                    <Button
                                                        Margin="0,4,0,0"
                                                        Command="{x:Static materialDesign:Flipper.FlipCommand}"
                                                        Content="INFO"
                                                        Style="{StaticResource MaterialDesignFlatButton}" />
                                                </StackPanel>
                                            </Grid>
                                        </materialDesign:Flipper.FrontContent>
                                        <materialDesign:Flipper.BackContent>
                                            <Grid
                                                MinWidth="200"
                                                MinHeight="256">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition
                                                        Height="Auto" />
                                                    <RowDefinition
                                                        Height="*" />
                                                </Grid.RowDefinitions>
                                                <materialDesign:ColorZone
                                                    Padding="6"
                                                    Mode="SecondaryMid">
                                                    <StackPanel
                                                        Orientation="Horizontal">
                                                        <Button
                                                            HorizontalAlignment="Left"
                                                            Command="{x:Static materialDesign:Flipper.FlipCommand}"
                                                            Style="{StaticResource MaterialDesignToolForegroundButton}">
                                                            <materialDesign:PackIcon
                                                                HorizontalAlignment="Right"
                                                                Kind="ArrowLeft" />
                                                        </Button>
                                                        <TextBlock
                                                            Margin="8,0,0,0"
                                                            VerticalAlignment="Center"
                                                            Text="{Binding AiSnake.Name}" />
                                                    </StackPanel>
                                                </materialDesign:ColorZone>
                                                <Grid
                                                    Grid.Row="1"
                                                    Width="172"
                                                    Margin="0,6,0,0"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Top">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                        <RowDefinition />
                                                    </Grid.RowDefinitions>
                                                    <TextBox
                                                        IsReadOnly="True"
                                                        Margin="0,12,0,0"
                                                        materialDesign:HintAssist.Hint="Poison type"
                                                        materialDesign:HintAssist.IsFloating="True"
                                                        Text="{Binding AiSnake.PoisonType.Name}" />
                                                    <TextBox IsReadOnly="True"
                                                             Grid.Row="1"
                                                             Margin="0,12,0,0"
                                                             materialDesign:HintAssist.Hint="Design"
                                                             materialDesign:HintAssist.IsFloating="True"
                                                             Text="{Binding AiSnake.Design.Name}" />

                                                    <GroupBox Grid.Row="2"
                                                              Margin="0,12,0,0"
                                                              materialDesign:ColorZoneAssist.Mode="SecondaryMid"
                                                              Header="Amount of Posion"
                                                              Style="{StaticResource MaterialDesignGroupBox}">
                                                        <TextBlock
                                                            materialDesign:HintAssist.Hint="Amount of Poison"
                                                            materialDesign:HintAssist.IsFloating="True"
                                                            Margin="0,12,0,0">
                                                            <Run Text="{Binding AiSnake.MinAmountOfPoison}" />
                                                            <Run Text="/" />
                                                            <Run Text="{Binding AiSnake.MaxAmountOfPoison}" />
                                                            <Run Text=" mg" />
                                                        </TextBlock>

                                                    </GroupBox>
                                                    <GroupBox Grid.Row="3"
                                                              Margin="0,12,0,0"
                                                              materialDesign:ColorZoneAssist.Mode="PrimaryMid"
                                                              Header="Lenght"
                                                              Style="{StaticResource MaterialDesignGroupBox}">
                                                        <TextBlock
                                                            materialDesign:HintAssist.Hint="Amount of Poison"
                                                            materialDesign:HintAssist.IsFloating="True"
                                                            Margin="0,12,0,0">
                                                            <Run Text="{Binding AiSnake.MinLenght}" />
                                                            <Run Text="/" />
                                                            <Run Text="{Binding AiSnake.MaxLenght}" />
                                                            <Run Text=" sm" />
                                                        </TextBlock>

                                                    </GroupBox>
                                                    <Expander
                                                        Grid.Row="4"
                                                        Margin="0,12,0,0"
                                                        HorizontalAlignment="Stretch"
                                                        Header="Areas">
                                                        <ListBox ItemsSource="{Binding AiSnake.Areas}"
                                                                 IsManipulationEnabled="False">
                                                            <ListBox.ItemTemplate>
                                                                <DataTemplate>
                                                                    <StackPanel Orientation="Horizontal">
                                                                        <TextBox IsReadOnly="True"
                                                                            Text="{Binding Name}" />
                                                                        <materialDesign:PackIcon
                                                                            Kind="MapMarker" />

<Window x:Class="Snakesssss.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        Title="MainWindow" Height="450" Width="900">


    <Grid>
        <materialDesign:Card>

            <TabControl
                VerticalContentAlignment="Bottom"
                materialDesign:ColorZoneAssist.Mode="PrimaryMid"
                Style="{StaticResource MaterialDesignNavigationRailTabControl}">
                <TabItem>
                    <TabItem.Header>
                        <StackPanel
                            Width="auto"
                            Height="auto">
                            <materialDesign:PackIcon
                                Width="50"
                                Height="50"
                                HorizontalAlignment="Center"
                                Kind="Snake" />
                            <TextBlock
                                HorizontalAlignment="Center"
                                Text="Змеи" />
                        </StackPanel>
                    </TabItem.Header>
                    <materialDesign:Card Padding="5">
                        <Grid>

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>

                            <Grid Column="1" Row="1">
                                <ListBox x:Name="SnakeList"
                                         Style="{StaticResource MaterialDesignGridList}"
                                         ItemsSource="{Binding Snakes}">
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <materialDesign:Card Padding="8" Margin="5">
                                                <materialDesign:Flipper
                                                    materialDesign:ElevationAssist.Elevation="Dp0"
                                                    Style="{StaticResource MaterialDesignCardFlipper}">
                                                    <materialDesign:Flipper.FrontContent>
                                                        <Grid
                                                            Width="200"
                                                            Height="306">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition
                                                                    Height="160" />
                                                                <RowDefinition
                                                                    Height="*" />
                                                            </Grid.RowDefinitions>
                                                            <materialDesign:ColorZone
                                                                VerticalAlignment="Stretch"
                                                                Mode="PrimaryLight">
                                                                <Image Source="{Binding ImagePath}" Width="160"
                                                                       Height="160" />
                                                            </materialDesign:ColorZone>
                                                            <StackPanel
                                                                Grid.Row="1"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center">
                                                                <StackPanel Orientation="Horizontal" Margin="2">
                                                                    <TextBox
                                                                        Text="{Binding Name}" FontWeight="Bold"
                                                                        FontStretch="Normal" />
                                                                </StackPanel>
                                                                <StackPanel Orientation="Horizontal" Margin="2">
                                                                    <TextBox
                                                                        materialDesign:HintAssist.Hint="Семейство"
                                                                        materialDesign:HintAssist.IsFloating="True"
                                                                        Text="{Binding Family.Name}" />
                                                                </StackPanel>

                                                                <Button
                                                                    Margin="0,4,0,0"
                                                                    Command="{x:Static materialDesign:Flipper.FlipCommand}"
                                                                    Content="Информация"
                                                                    Style="{StaticResource MaterialDesignFlatButton}" />
                                                            </StackPanel>
                                                        </Grid>
                                                    </materialDesign:Flipper.FrontContent>
                                                    <materialDesign:Flipper.BackContent>
                                                        <Grid
                                                            MinWidth="200"
                                                            MinHeight="256">
                                                            <Grid.RowDefinitions>
                                                                <RowDefinition
                                                                    Height="Auto" />
                                                                <RowDefinition
                                                                    Height="*" />
                                                            </Grid.RowDefinitions>
                                                            <materialDesign:ColorZone
                                                                Padding="6"
                                                                Mode="SecondaryMid">
                                                                <StackPanel
                                                                    Orientation="Horizontal">
                                                                    <Button
                                                                        HorizontalAlignment="Left"
                                                                        Command="{x:Static materialDesign:Flipper.FlipCommand}"
                                                                        Style="{StaticResource MaterialDesignToolForegroundButton}">
                                                                        <materialDesign:PackIcon
                                                                            HorizontalAlignment="Right"
                                                                            Kind="ArrowLeft" />
                                                                    </Button>
                                                                    <TextBlock
                                                                        Margin="8,0,0,0"
                                                                        VerticalAlignment="Center"
                                                                        Text="{Binding Name}" />
                                                                </StackPanel>
                                                            </materialDesign:ColorZone>
                                                            <Grid
                                                                Grid.Row="1"
                                                                Width="172"
                                                                Margin="0,6,0,0"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Top">
                                                                <Grid.RowDefinitions>
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                    <RowDefinition />
                                                                </Grid.RowDefinitions>
                                                                <TextBox
                                                                    IsReadOnly="True"
                                                                    Margin="0,12,0,0"
                                                                    materialDesign:HintAssist.Hint="Тип яда"
                                                                    materialDesign:HintAssist.IsFloating="True"
                                                                    Text="{Binding PoisonType.Name}" />
                                                                <TextBox IsReadOnly="True"
                                                                         Grid.Row="1"
                                                                         Margin="0,12,0,0"
                                                                         materialDesign:HintAssist.Hint="Узор"
                                                                         materialDesign:HintAssist.IsFloating="True"
                                                                         Text="{Binding Design.Name}" />

                                                                <GroupBox Grid.Row="2"
                                                                          Margin="0,12,0,0"
                                                                          materialDesign:ColorZoneAssist.Mode="SecondaryMid"
                                                                          Header="Количество яда"
                                                                          Style="{StaticResource MaterialDesignGroupBox}">
                                                                    <TextBlock
                                                                        materialDesign:HintAssist.Hint="Количество яда"
                                                                        materialDesign:HintAssist.IsFloating="True"
                                                                        Margin="0,12,0,0">
                                                                        <Run Text="{Binding MinAmountOfPoison}" />
                                                                        <Run Text="/" />
                                                                        <Run Text="{Binding MaxAmountOfPoison}" />
                                                                        <Run Text=" мл" />
                                                                    </TextBlock>

                                                                </GroupBox>
                                                                <GroupBox Grid.Row="3"
                                                                          Margin="0,12,0,0"
                                                                          materialDesign:ColorZoneAssist.Mode="PrimaryMid"
                                                                          Header="Длина"
                                                                          Style="{StaticResource MaterialDesignGroupBox}">
                                                                    <TextBlock
                                                                        materialDesign:HintAssist.Hint="Amount of Poison"
                                                                        materialDesign:HintAssist.IsFloating="True"
                                                                        Margin="0,12,0,0">
                                                                        <Run Text="{Binding MinLenght}" />
                                                                        <Run Text="/" />
                                                                        <Run Text="{Binding MaxLenght}" />
                                                                        <Run Text=" см" />
                                                                    </TextBlock>

                                                                </GroupBox>
                                                                <Expander
                                                                    Grid.Row="4"
                                                                    Margin="0,12,0,0"
                                                                    HorizontalAlignment="Stretch"
                                                                    Header="Ареал">
                                                                    <ListBox ItemsSource="{Binding Areas}"
                                                                             IsManipulationEnabled="False">
                                                                        <ListBox.ItemTemplate>
                                                                            <DataTemplate>
                                                                                <StackPanel Orientation="Horizontal">
                                                                                    <TextBox IsReadOnly="True"
                                                                                        Text="{Binding Name}" />
                                                                                    <materialDesign:PackIcon
                                                                                        Kind="MapMarker" />
                                                                                </StackPanel>

                                                                            </DataTemplate>
                                                                        </ListBox.ItemTemplate>
                                                                    </ListBox>
                                                                </Expander>
                                                                <Expander
                                                                    Grid.Row="5"
                                                                    Margin="0,12,0,0"
                                                                    HorizontalAlignment="Stretch"
                                                                    Header="Цвета">
                                                                    <ListBox ItemsSource="{Binding Colors}"
                                                                             Style="{StaticResource MaterialDesignGridList}"
                                                                             IsManipulationEnabled="False">
                                                                        <ListBox.ItemTemplate>
                                                                            <DataTemplate>
                                                                                <Ellipse Width="20" Height="20"
                                                                                    Fill="{Binding Color}" Margin="5" />
                                                                            </DataTemplate>
                                                                        </ListBox.ItemTemplate>
                                                                    </ListBox>
                                                                </Expander>
                                                                <StackPanel Grid.Row="6" Orientation="Vertical"
                                                                            HorizontalAlignment="Center">
                                                                    <TextBlock>Рейтинг опасности</TextBlock>
                                                                    <materialDesign:RatingBar
                                                                        IsPreviewValueEnabled="True"
                                                                        Max="5"
                                                                        Min="0"
                                                                        ValueIncrements="0.25"
                                                                        Value="{Binding DangerausScore}"
                                                                        PreviewMouseDown="UIElement_OnPreviewMouseDown" />

                                                                </StackPanel>
                                                                <StackPanel Grid.Row="7" Orientation="Horizontal"
                                                                            HorizontalAlignment="Right">
                                                                    <Button
                                                                        IsEnabled="{Binding Path = DataContext.User.CanEdit, Source={x:Reference Name=SnakeList}}"
                                                                        Margin="5"
                                                                        HorizontalAlignment="Right"
                                                                        Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}"
                                                                        ToolTip="MaterialDesignFloatingActionMiniLightButton"
                                                                        Command="{Binding Path = DataContext.EditSnake, Source={x:Reference Name=SnakeList}}"
                                                                        CommandParameter="{Binding .}">
                                                                        <materialDesign:PackIcon
                                                                            Width="20"
                                                                            Height="20"
                                                                            Kind="Pencil" />
                                                                    </Button>
                                                                    <Button
                                                                        IsEnabled="{Binding Path = DataContext.User.CanEdit, Source={x:Reference Name=SnakeList}}"
                                                                        materialDesign:ColorZoneAssist.Mode="Dark"
                                                                        Margin="5"
                                                                        Command="{Binding Path = DataContext.DeleteSnake, Source={x:Reference Name=SnakeList}}"
                                                                        CommandParameter="{Binding .}"
                                                                        HorizontalAlignment="Right"
                                                                        Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}"
                                                                        ToolTip="MaterialDesignFloatingActionMiniLightButton">
                                                                        <materialDesign:PackIcon
                                                                            Width="35"
                                                                            Height="35"
                                                                            Kind="CloseCircle" />
                                                                    </Button>

                                                                </StackPanel>


                                                            </Grid>
                                                        </Grid>
                                                    </materialDesign:Flipper.BackContent>
                                                </materialDesign:Flipper>
                                            </materialDesign:Card>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>

                                </ListBox>
                                <!--<ListBox  SelectedItem="{Binding SelectedSnake}" materialDesign:ListBoxItemAssist.ShowSelection="True"
                                         ItemsSource="{Binding Snakes}">

                                    <ListBox.ItemContainerStyle>
                                        <Style TargetType="ListBoxItem"
                                               BasedOn="{StaticResource MaterialDesignListBoxItem}">
                                            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
                                            

                                        </Style>
                                    </ListBox.ItemContainerStyle>
                                    <ListBox.ItemTemplate>
                                        
                                        <DataTemplate>

                                            <materialDesign:Card Padding="4" Style="{StaticResource  MaterialDesignFlatButton}"  >
                                                <materialDesign:Card.InputBindings>
                                                    <MouseBinding MouseAction="LeftDoubleClick"
                                                                  Command="{Binding DataContext.LoadSpectralClasses, RelativeSource={RelativeSource AncestorType={x:Type ListBox}}}"/>
